    "@tanbo/color-picker": "^1.0.3",
    "@tanbo/css-themes": "^0.0.1",
    "@tanbo/di": "^1.1.5",
    "@tanbo/stream": "^1.2.0",
    "@textbus/core": "^3.7.1",
    "@textbus/platform-browser": "^3.7.2",
